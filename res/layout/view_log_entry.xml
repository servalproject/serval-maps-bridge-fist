<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/smapsLayoutmatch"
    android:orientation="vertical" 
    android:id="@+id/view_log_ui_entry">
    
     <TextView
        android:id="@+id/view_log_ui_entry_title"
        style="@style/smapsLabel" />
     
     <TableLayout
        style="@style/smapsLayout" >

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/smapsWrap" >
            
            <TextView
		        android:id="@+id/view_log_ui_entry_status_lbl"
		        style="@style/smapsLabel" 
		        android:text="@string/view_log_ui_entry_status_lbl"/>
		    
		    <TextView
		        android:id="@+id/view_log_ui_entry_status_txt"
		        style="@style/smapsTextwrapped" />
		    
        </TableRow>
        
        <TableRow
            android:id="@+id/tableRow2"
            style="@style/smapsWrap" >
		    
            <TextView
		        android:id="@+id/view_log_ui_entry_status_last_updated_lbl"
		        style="@style/smapsLabel" 
		        android:text="@string/view_log_ui_entry_status_last_updated_lbl" />
		
		    <TextView
		        android:id="@+id/view_log_ui_entry_status_last_updated_txt"
		        style="@style/smapsTextwrapped" 
		        android:text="" />
    
        </TableRow>
    </TableLayout>
    
    
    <View 
      android:id="@+id/Separator"
	  style="@style/smapsLayout"
	  android:background="?android:attr/listDivider" />
</LinearLayout>